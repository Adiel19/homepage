body{
	background-color: #EFFFDA;
	padding-top: 100px;
	width: 100%;
	height: 100%;
}

.col-center{
	float: none;
	margin: 0 auto;
	padding-top: 6%;
}
.navbar-default .navbar-nav>li>a{
	color: #005b96;
}

.navbar-nav > li > a, .navbar-brand {
    padding-top:4px !important; 
    padding-bottom:0 !important;
    height: 28px;
}
.navbar {max-height:28px !important;}

.wrapper{
	padding-left: 15px;
	padding-right: 15px;
}

.border_shadow{
  border: 2px #e4e4e4 solid;
  padding: 0px;
  border-radius: 4px;
  box-shadow: 0 0 10px #ccc;
  background-color: #ffffff;
}
h1{
  font-family: 'Pacifico', cursive;
  font-size: 350%;
  color:  #daf1ff;
  text-align: center;
}
h2{
	font-family: 'Dancing Script', cursive;
	font-size: 200%;
	color:#f0f9ff;
	text-align: center;
}
h3{
  font-family: 'Dancing Script', cursive;
  font-size: 150%;
  color: #f0f9ff;
  text-align: center;
}

